{
	"name": "Golden Era",
	"type": "dark",
	"colors": {
		// editor
		"editor.background": "#193549",
		"editor.foreground": "#00ffff",
		"editorCursor.foreground": "#ffc410",
		"editor.lineHighlightBackground": "#091f2f",
		"editor.lineHighlightBorder": "#4f6b80",
		// error
		"errorForeground": "#ff0000",
		"editorError.border": "#ff0000",
		// activity bar badge
		"activityBarBadge.background": "#fff",
		"activityBar.background": "#193549",
		"activityBar.foreground": "#ffc410",
		"window.activeBorder": "#ffc410",
		"editorBracketMatch.background": "#193549",
		// statusbar
		"statusBar.background": "#193549",
		"statusBar.foreground": "#ffc410",
		"statusBar.border": "#ffc410",
		// side bar
		"sideBar.background": "#091f2f",
		"sideBar.border": "#ffc410",
		"sideBar.foreground": "#ffc410",
		"sideBarTitle.foreground": "#ffc410",
		// menubar
		"menu.background": "#193549",
		"menubar.selectionForeground": "#ffc410",
		"menubar.selectionBackground": "#193549",
		"menubar.selectionBorder": "#ffc410",
		// notification
		"notifications.border": "#ffc410",
		"notificationsErrorIcon.foreground": "#f5eeee",
		// terminal
		"terminal.border": "#ffc410",
		"terminal.foreground": "#ffc410",
		"terminal.selectionBackground": "#2646d6",
		// input
		"input.border": "#ffc410",
		"input.foreground": "#00e9e9",
		"input.placeholderForeground": "#fff",
		// scrollbar
		"scrollbarSlider.background": "#ffc410",
		"scrollbarSlider.hoverBackground": "#ffc410",
		"scrollbarSlider.activeBackground": "#ffc410",
		"scrollbar.shadow": "#ffc410",
		// mini map
		"minimap.background": "#193549",
		// text link color --on welcome page
		"textLink.foreground": "#ffc410",
		"textLink.activeForeground": "#ffc410",
		// tab
		"tab.activeBorder": "#ffc410",
		"tab.hoverBorder": "#ffc410",
		"tab.activeForeground": "#ffc410",
		// editor line number
		"editorLineNumber.foreground": "#ffc410",
		"editorLineNumber.activeForeground": "#ffc410",
		// dropdown background
		"dropdown.background": "#091f2f",
		"dropdown.foreground": "#ffc410"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#a4a2a5"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"text.html.derivative",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#fff"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#00e9e9"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffc410"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#07b9ff"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"punctuation.separator.inheritance.php",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#ff9d00"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"meta.tag",
				"punctuation.definition.tag",
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#21ECF3"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#ffcb6b"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "constant Numeric",
			"scope": [
				"constant.numeric",
			],
			"settings": {
				"foreground": "#ff628c"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#A5FF90"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#FFCB6B"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
